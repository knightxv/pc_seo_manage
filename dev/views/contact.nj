{% extends "layout/template.nj" %}
{% import "component/index.nj" as component %}

{% block css %}
<link rel="stylesheet" href="{{ static }}/css/contact.css">
{% endblock %}

{% set breadArr = [{label: '首页', href: '/'}, {label: '联系我们', href: ''}] %}

{% block main %}
{{ component.headBanner(5) }}
{{ component.BannerImg(static + "/images/banner.jpg") }}
{{ component.Breadcrumb(breadArr) }}
<section class="container">
    <section class="col-md-3">
		<div class="m_module">
			{{ component.Title('联系我们') }}
			{{ component.SideSelect([{label: '联系我们', href: ''}]) }}
		</div>
	</section>
	<section class="col-md-8 m_module contact_module">
		{{ htmlText }}
	</section>
</section>
{% endblock %}
